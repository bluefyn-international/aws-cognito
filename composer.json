{
    "name": "bluefyn-international/aws-cognito",
    "description": "AWS Cognito package that allows Auth and other related features using the AWS SDK for PHP",
    "keywords": ["php", "laravel", "aws", "cognito", "auth", "authentication", "oauth", "user pool"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/bluefyn-international/aws-cognito",
    "authors": [
        {
            "name": "Bluefyn International",
            "role": "owner"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": ">=6.0",
        "aws/aws-sdk-php": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "friendsofphp/php-cs-fixer": "^2.17"
    },
    "autoload": {
        "psr-4": {
          "Ellaisys\\Cognito\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ellaisys\\Cognito\\Providers\\AwsCognitoServiceProvider"
            ],
            "aliases": {
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
